<h3>Index</h3>

<p>{{link-to 'New choice' 'choices.new'}}</p>

<table>
  <tr>
    <th>id</th>
    <th>Word</th>
    <th>Correct</th>
    <th>Card</th>
  </tr>
  {{#each}}
    <tr>
      <td>{{link-to id 'choices.show' this}}</td>
      <td>{{word}}</td>
      <td>{{correct}}</td>
      <td>{{card}}</td>
    </tr>
  {{/each}}
</table>
